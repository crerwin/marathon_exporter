{
    "id": "/marathonexporter",
    "backoffFactor": 1.15,
    "backoffSeconds": 1,
    "cmd": "/marathon_exporter --marathon.uri=[dcosadminrouter]/marathon --dcos.serviceaccountuid=testsa --dcos.serviceaccountprivatekeyfile=$MESOS_SANDBOX/testsa.pem",
    "container": {
      "portMappings": [
        {
          "containerPort": 9088,
          "hostPort": 0,
          "labels": {
            "VIP_0": "/marathonexporter:9088"
          },
          "protocol": "tcp",
          "servicePort": 10000,
          "name": "marathonexporter"
        }
      ],
      "type": "MESOS",
      "volumes": [
        {
          "containerPath": "testsa.pem",
          "secret": "testsa_private_key"
        }
      ],
      "docker": {
        "image": "crerwin/marathon_exporter",
        "forcePullImage": false,
        "parameters": []
      }
    },
    "cpus": 0.1,
    "disk": 0,
    "instances": 1,
    "maxLaunchDelaySeconds": 3600,
    "mem": 128,
    "gpus": 0,
    "networks": [
      {
        "mode": "container/bridge"
      }
    ],
    "requirePorts": false,
    "secrets": {
      "testsa_private_key": {
        "source": "testsa_private_key"
      }
    },
    "upgradeStrategy": {
      "maximumOverCapacity": 1,
      "minimumHealthCapacity": 1
    },
    "killSelection": "YOUNGEST_FIRST",
    "unreachableStrategy": {
      "inactiveAfterSeconds": 0,
      "expungeAfterSeconds": 0
    },
    "healthChecks": [],
    "fetch": [],
    "constraints": []
  }